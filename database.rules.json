          "x": { ".validate": "newData.isNumber()"}, 
          "y": { ".validate": "newData.isNumber()"}, 
          "xp": { ".validate": "newData.isNumber()"}, 
          "cash": { ".validate": "newData.isNumber()"},
          "critdamage": { ".validate": "newData.isNumber()"}, 
          "going_x": { ".validate": "newData.isNumber()"}, 
          "going_y": { ".validate": "newData.isNumber()"}, 
          "move_num": { ".validate": "newData.isNumber()"}, 
          "moving": { ".validate": "newData.isBoolean()"}, 
          "type": { ".validate": "newData.val() === 'mage' || newData.val()  === 'priest' || newData.val() === 'rogue' || newData.val()  === 'paladin' || newData.val()  === 'merchant' || newData.val()  === 'warrior' || newData.val()  === 'ranger'"},
          "$other": { ".validate": false }
        },
        "$other": { ".validate": false }
      }
    }
