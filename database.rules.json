          "gold": { ".validate": "newData.isNumber()"}, 
          "goldm": { ".validate": "newData.isNumber()"}, 
          "i": { ".validate": "newData.isNumber()"}, 
          "in": { ".validate": "newData.isString()"}, 
          "int": { ".validate": "newData.isNumber()"}, 
          "isize": { ".validate": "newData.isNumber()"}, 
          "items": { ".validate": true}, 
          "j": { ".validate": "newData.isNumber()"}, 
          "last_fear": { ".validate": "newData.isNumber()"}, 
