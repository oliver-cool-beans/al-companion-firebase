{
  "rules": {
    "users": {
    	"$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid",
        "$other": { ".validate": false }
      }
    },
    "characters": {
      "$character":{
        ".read": "auth != null && (data.child('uid').val() === auth.uid || data.child('isPublic').val() === true)", 
        ".write": "auth != null && (!data.exists() || data.child('uid').val() === auth.uid)",
        "uid": { ".validate": "newData.val() === auth.uid"},
        "updatedAt": { ".validate": true },
        "isPublic": { ".validate": "newData.isBoolean()" },
        "data":{
          "acx": { ".validate": true },
          "age": { ".validate": "newData.isNumber()"}, 
          "aheight": { ".validate": "newData.isNumber()"},
          "awidth": { ".validate": "newData.isNumber()"},  
          "alpha": { ".validate": "newData.isNumber()"}, 
          "animations": { ".validate": true}, 
          "apiercing": { ".validate": "newData.isNumber()"}, 
          "armor": { ".validate": "newData.isNumber()"}, 
          "attack": { ".validate": "newData.isNumber()"}, 
          "blast": { ".validate": "newData.isNumber()"}, 
          "cc": { ".validate": "newData.isNumber()"}, 
